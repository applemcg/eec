# mk -- make and test current cummings
(
echo "proc eec_wordlist {} {  return {wordlist "
cat eecmachine.tcl |
     sed -n '/^# BEGIN visible/,/^# END visible/p' |
     awk '$1 ~ /proc/ { print $2 }' | 
     sed 's/eec_/, /' | sort 
     echo "} }"
) > eecwords.tcl

 f=firstCircle
 f=listExercise

tc=any
tc=flag
tc=file

case $tc in 

file)  ./eec $f.eec 2> $f.err | tee $f.out ;;
flag)  ./eec -f foo;;
*)     ./eec $f.eec;;

esac

