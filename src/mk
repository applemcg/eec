(
echo "proc eec_wordlist {} {  return {wordlist "
cat eecparse.tcl |
     sed -n '/^# BEGIN visible/,/^# END visible/p' |
     awk '$1 ~ /proc/ { print $2 }' | 
     sed 's/eec_/, /' | sort 
     echo "} }"
 

) > eecwords.tcl


 f=firstCircle
 f=listExercise


 ./eec $f.eec 2> $f.err | tee $f.out ;

