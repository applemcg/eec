function( factorial,
    docstring(
        returns the factorial of a
	non-negative integer argument
    )
    args( n),
    return( 
 	 if( 1st less than 2nd (n, 2), 1,
	     multiply( n,
     	       factorial( subtract 2nd from 1st ( n, 1))
             )
          )
     )
)
function( append space,
	  args( msg),
	  return( concatenate( msg, \ ))
)
function ( as, args( m), return( append space( m)))

print  ( concatenate( 

       as( factorial of), as( str( n)), factorial( n)
))


